{
  "name": "nest-vite-node-starter",
  "version": "0.0.0-dev",
  "description": "",
  "private": true,
  "scripts": {
    "dev": "node node_modules/vite/bin/vite.js",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "npm run eslint && npm run tscheck && npm run test:cov && npm run test:e2e",
    "test:r": "jest --runInBand",
    "test:cov": "npm run test:r -- --coverage",
    "test:w": "npm run test:r -- --watch",
    "eslint": "node node_modules/eslint/bin/eslint \"src/**/*.{ts,tsx}\"",
    "eslint:w": "watchexec -w src \"npm run eslint\"",
    "eslint:fix": "npm run eslint -- --fix",
    "tscheck": "tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "commit": "cz",
    "build": "sh Taskfile build",
    "build_wp": "sh Taskfile build_wp",
    "test:e2e": "jest --runInBand --config ./test/jest-e2e.config.js",
    "test:d": "ndb -r @swc/register node_modules/jest/bin/jest.js --runInBand --watch"
  },
  "dependencies": {
    "@nestjs/common": "^8.2.4",
    "@nestjs/core": "^8.2.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.6",
    "@nestjs/microservices": "^8.2.4",
    "@nestjs/platform-express": "^8.2.4",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.2.4",
    "@nestjs/websockets": "^8.2.4",
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.124",
    "@swc/helpers": "^0.3.2",
    "@swc/jest": "^0.2.15",
    "@swc/register": "^0.1.9",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "condition-loader": "^1.0.5",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unicorn": "^40.0.0",
    "expect": "^27.4.2",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.1",
    "supertest": "^6.1.6",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "tsconfig-paths-jest-mapper": "^1.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "vite": "^2.7.10",
    "vite-plugin-node": "^0.0.15",
    "watchexec-bin": "^1.0.0",
    "webpack": "^5.65.0"
  }
}
