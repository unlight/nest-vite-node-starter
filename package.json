{
  "name": "nest-vite-node-starter",
  "version": "0.0.0-dev",
  "description": "",
  "private": true,
  "scripts": {
    "dev": "node node_modules/vite/bin/vite.js",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "npm run eslint && npm run tscheck && npm run test:cov && npm run test:e2e",
    "test:p": "ultra test",
    "test:r": "jest --runInBand",
    "test:cov": "npm run test:r -- --coverage",
    "test:w": "npm run test:r -- --watch",
    "lint": "npm run eslint && npm run tscheck",
    "eslint": "node node_modules/eslint/bin/eslint \"src/**/*.{ts,tsx}\"",
    "eslint:w": "watchexec -w src \"npm run eslint\"",
    "eslint:fix": "npm run eslint -- --fix",
    "tscheck": "tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "lint:fix": "npm run eslint:fix",
    "format:src": "prettier src --write",
    "commit": "cz",
    "build": "sh Taskfile build",
    "lint:w": "run-p tscheck:w eslint:w",
    "test:e2e": "jest --runInBand --config ./test/jest-e2e.config.js",
    "test:d": "ndb -r @swc/register node_modules/jest/bin/jest.js --runInBand --watch"
  },
  "ultra": {
    "concurrent": [
      "lint",
      "test"
    ]
  },
  "dependencies": {
    "@nestjs/common": "^8.0.6",
    "@nestjs/core": "^8.0.6",
    "@nestjs/platform-express": "^8.0.6",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.3.0"
  },
  "devDependencies": {
    "@abraham/reflection": "^0.8.0",
    "@nestjs/cli": "^8.1.1",
    "@nestjs/schematics": "^8.0.3",
    "@nestjs/testing": "^8.0.6",
    "@swc/cli": "^0.1.49",
    "@swc/core": "^1.2.84",
    "@swc/helpers": "^0.2.13",
    "@swc/jest": "^0.2.2",
    "@swc/register": "^0.1.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.10",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "condition-loader": "^1.0.5",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-etc": "^1.5.4",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-regexp": "^1.1.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.10.0",
    "eslint-plugin-sort-class-members": "^1.11.0",
    "eslint-plugin-total-functions": "^4.10.1",
    "eslint-plugin-unicorn": "^35.0.0",
    "eslint-plugin-wix-editor": "^3.3.0",
    "expect": "^27.1.0",
    "git-branch-is": "^4.0.0",
    "jest": "^27.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "simplytyped": "^3.3.0",
    "supertest": "^6.1.6",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "tsconfig-paths-jest-mapper": "^1.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.2",
    "ultra-runner": "^3.10.5",
    "vite": "^2.5.3",
    "vite-plugin-node": "^0.0.14",
    "watchexec-bin": "^1.0.0"
  }
}
