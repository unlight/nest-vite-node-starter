{
  "name": "nest-vite-node-starter",
  "version": "0.0.0-dev",
  "description": "",
  "private": true,
  "scripts": {
    "dev": "node node_modules/vite/bin/vite.js",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "npm run eslint && npm run tscheck && npm run test:cov && npm run test:e2e",
    "test:r": "jest --runInBand",
    "test:cov": "npm run test:r -- --coverage",
    "test:w": "npm run test:r -- --watch",
    "eslint": "node node_modules/eslint/bin/eslint \"src/**/*.{ts,tsx}\"",
    "eslint:w": "watchexec -w src \"npm run eslint\"",
    "eslint:fix": "npm run eslint -- --fix",
    "tscheck": "tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "commit": "cz",
    "build": "sh Taskfile build_wp",
    "vite:build": "sh Taskfile vite_build",
    "test:e2e": "jest --runInBand --config ./test/jest-e2e.config.js",
    "test:d": "ndb -r @swc/register node_modules/jest/bin/jest.js --runInBand --watch"
  },
  "dependencies": {
    "@nestjs/common": "^8.4.3",
    "@nestjs/core": "^8.4.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.4",
    "@nestjs/platform-express": "^8.4.3",
    "@nestjs/schematics": "^8.0.9",
    "@nestjs/testing": "^8.4.3",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.163",
    "@swc/helpers": "^0.3.8",
    "@swc/jest": "^0.2.20",
    "@swc/register": "^0.1.10",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "env-bool": "^2.0.1",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "expect": "^27.5.1",
    "jest": "^27.5.1",
    "load-json-file": "6",
    "prettier": "^2.6.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5",
    "supertest": "^6.2.2",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "tsconfig-paths-jest-mapper": "^1.4.0",
    "tsconfig-to-swcconfig": "^1.5.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3",
    "vite": "^2.9.1",
    "vite-plugin-node": "^0.0.19",
    "vite-tsconfig-paths": "^3.4.1",
    "watchexec-bin": "^1.0.0",
    "webpack": "^5.71.0"
  }
}
